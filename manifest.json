{
  "manifest_version": 3,
  "name": "旧字体化",
  "version": "1.0",
  "description": "Webページ上の漢字を旧字体にします。",
  "homepage_url": "https://github.com/MeyMey-lab/kyujitai",
  "permissions": ["activeTab","storage"],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "icons": {
    "48": "icons/48.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["data.js", "content.js"],
      "run_at": "document_end"
    }
  ]
}
